{
  "private": true,
  "name": "cardinalscout",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src",
    "pretty": "prettier ./.prettierrc --write \"src/**/**.js\" ",
    "flow": "flow-typed run-tests"
  },
  "browserslist": [
    "> 1%",
    "IE >= 9",
    "last 2 versions"
  ],
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-synacor": "^2.0.4",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-prettier": "^2.4.0",
    "flow-bin": "^0.63.1",
    "if-env": "^1.0.0",
    "jest": "^22.1.1",
    "preact-cli": "^2.0.0",
    "preact-cli-plugin-async": "^1.0.0",
    "preact-cli-plugin-flow": "^0.2.0",
    "prettier": "^1.10.2",
    "redux-devtools": "^3.4.1",
    "redux-devtools-log-monitor": "^1.4.0"
  },
  "dependencies": {
    "firebase": "4.8.0",
    "immutable": "^3.8.2",
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "preact-material-components": "^1.2.4",
    "react-redux": "^5.0.6",
    "react-redux-firebase": "^2.0.0",
