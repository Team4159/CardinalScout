{
  "private": true,
  "name": "cardinalscout",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --no-prerender",
    "serve": "preact build --no-prerender && preact serve",
    "deploy": "preact build --no-prerender && firebase deploy",
    "dev": "preact watch",
    "lint": "eslint src",
    "pretty": "prettier ./.prettierrc --write \"src/**/**.js\" ",
    "flow": "flow"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "eslintIgnore": [
    "build/*"
  ],
  "performance": {
    "maxAssetSize": 650000,
    "maxEntrypointSize": 650000
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.37",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-synacor": "^2.0.4",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-prettier": "^2.4.0",
    "flow-bin": "^0.63.1",
    "if-env": "^1.0.0",
    "jest": "^22.1.1",
    "node-sass": "^4.7.2",
    "preact-cli": "^2.0.0",
    "preact-cli-plugin-async": "^1.0.0",
    "preact-cli-plugin-flow": "^0.2.0",
    "prettier": "^1.10.2",
    "sass-loader": "^6.0.6",
    "webpack-bundle-analyzer": "^2.9.2"
  },
  "dependencies": {
    "firebase": "4.8.0",
    "history": "^4.7.2",
    "localforage": "^1.5.6",
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "preact-css-transition-group": "^1.3.0",
    "preact-material-components": "^1.3.5",
    "react-redux": "^5.0.6",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-persist": "^5.5.0",
    "redux-saga": "^0.16.0",
    "redux-saga-firebase": "^0.9.0",
    "reselect": "^3.0.1"
  }
}
